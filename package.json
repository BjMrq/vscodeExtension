{
	"name": "spockee",
	"displayName": "Spockee",
	"publisher": "bjmrqspockee",
	"description": "Extension to use with CLI",
	"repository": {
		"type": "git",
		"url": "https://github.com/BjMrq/vscodeExtension"
	},
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:spockeeExplorer",
		"onView:spockeeCli",
		"onView:spockeeDockerState",
		"onView:spockeeDockerGroup",
		"onView:installedApplications",
		"onView:spockeeApplications",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "spockeeExplorer",
					"title": "Spockee",
					"icon": "media/spockeeIcon.png"
				}
			]
		},
		"views": {
			"spockeeExplorer": [
				{
					"id": "spockeeCli",
					"name": "CLI commands"
				},
				{
					"id": "spockeeDockerState",
					"name": "Spockee State"
				},
				{
					"id": "spockeeDockerGroup",
					"name": "Docker Groups"
				},
				{
					"id": "installedApplications",
					"name": "Installed applications"
				},
				{
					"id": "spockeeApplications",
					"name": "Spockee applications"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "spockeeStateData.refreshEntry",
					"when": "view == spockeeDockerState",
					"group": "navigation"
				},
				{
					"command": "spockeeApplicationData.refreshEntry",
					"when": "view == spockeeApplications",
					"group": "navigation"
				},
				{
					"command": "spockeeApplicationData.refreshEntry",
					"when": "view == installedApplications",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "spockeeApp.openInCode",
					"when": "view == installedApplications && viewItem == installedApplication"
				},
				{
					"command": "spockeeApp.install",
					"when": "view == spockeeApplications && viewItem == spockeeApplication"
				}
			]
		},
		"commands": [
			{
				"command": "spockeeApplicationData.refreshEntry",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "spockeeStateData.refreshEntry",
				"title": "Refresh",
				"icon": "$(refresh)"
			},
			{
				"command": "spockeeApp.openInCode",
				"title": "Open in code",
				"icon": "$(code)"
			},
			{
				"command": "spockeeApp.install",
				"title": "Install",
				"icon": "$(install)"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "IS_DEV=true tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/shelljs": "^0.8.8",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-hardcore": "^13.2.0",
		"eslint-config-standard": "16.0.2",
		"eslint-plugin-functional": "^3.2.1",
		"eslint-plugin-unicorn": "^28.0.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"@bjmrq/utils": "0.0.23",
		"@types/ramda": "^0.27.38",
		"dotenv": "^8.2.0",
		"ramda": "^0.27.1",
		"shelljs": "^0.8.4"
	}
}